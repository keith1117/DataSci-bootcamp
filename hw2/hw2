import numpy as np

// Q1
A = np.array([[1, 2, 3], [4, 5, 6]])
B = np.array([[7, 8, 9], [10, 11, 12]])

print("Array A:\n", A)
print("Array B:\n", B)

vertical_stack = np.vstack((A, B))
print("Vertical Stack Result:\n", vertical_stack)

horizontal_stack = np.hstack((A, B))
print("Horizontal Stack Result:\n", horizontal_stack)

// Q2
